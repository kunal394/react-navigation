{
  "name": "@ashishsaundekar/react-navigation",
  "version": "2.18.6",
  "description": "Routing and navigation for your React Native apps",
  "main": "src/react-navigation.js",
  "repository": {
    "url": "git+ssh://git@github.com/ashishsaundekar/react-navigation.git",
    "type": "git"
  },
  "author": "Adam Miskiewicz <adam@sk3vy.com>",
  "license": "BSD-2-Clause",
  "type":"module",
  "scripts": {
    "start": "npm run ios",
    "ios": "cd examples/NavigationPlayground && yarn && yarn ios",
    "android": "cd examples/NavigationPlayground && yarn && yarn android",
    "test": "npm run lint && npm run jest",
    "codecov": "codecov",
    "jest": "jest",
    "test-update-snapshot": "jest --updateSnapshot",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "precommit": "lint-staged"
  },
  "files": [
    "src",
    "NavigationTestUtils.js"
  ],
  "peerDependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "create-react-context": "0.3.0",
    "hoist-non-react-statics": "3.3.2",
    "path-to-regexp": "^1.7.0",
    "query-string": "^6.1.0",
    "react-is": "^16.13.1",
    "react-lifecycles-compat": "^3",
    "react-native-screens": "1.0.0-alpha.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-flow": "7.9.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.3.1",
    "codecov": "^3.8.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "jest-expo": "^42.1.0",
    "lint-staged": "^11.2.3",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.4",
    "prettier-eslint": "^13.0.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-native": "^0.66.1",
    "react-native-vector-icons": "^8.1.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "notify": true,
    "preset": "react-native",
    "testRegex": "/__tests__/[^/]+-test\\.js$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.png$": "<rootDir>/assetsTransformer.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/examples/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|react-navigation-deprecated-tab-navigator|react-navigation-stack)"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write flow/react-navigation.js",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/ashishsaundekar/react-navigation/issues"
  },
  "homepage": "https://github.com/ashishsaundekar/react-navigation#readme",
  "directories": {
    "example": "examples"
  },
  "keywords": [
    "navigation"
  ]
}
